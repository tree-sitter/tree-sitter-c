{
  "grammars": [
    {
      "name": "c",
      "camelcase": "C",
      "scope": "source.c",
      "path": ".",
      "file-types": [
        "c",
        "h"
      ],
      "highlights": "queries/highlights.scm",
      "tags": "queries/tags.scm",
      "injection-regex": "^(c|h)$"
    }
  ],
  "metadata": {
    "version": "0.24.1",
    "license": "MIT",
    "description": "C grammar for tree-sitter",
    "authors": [
      {
        "name": "Max Brunsfeld",
        "email": "maxbrunsfeld@gmail.com"
      },
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-c"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
